{
  "name": "tahitispeak",
  "version": "1.0.0",
  "description": "TahitiSpeak - Interactive Tahitian Language Learning Platform",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rubling66/TahitiSpeak.git"
  },
  "homepage": "https://github.com/Rubling66/TahitiSpeak",
  "author": "Rubling66",
  "keywords": ["tahitian", "language-learning", "education", "nextjs", "react"],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rubling66/TahitiSpeak/issues"
  },
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint",
    "check:env": "node scripts/check-env.js",
    "test:apis": "node scripts/test-apis.js",
    "health-check": "node scripts/health-check.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:api": "jest --testPathPatterns=tests/api",
    "test:integration": "jest --testPathPatterns=tests/integration",
    "test:all": "npm run test:unit && npm run test:api",
    "test:ci": "jest --coverage --watchAll=false --testPathPatterns=tests/unit",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "lhci autorun",
    "test:accessibility": "axe http://localhost:3000",
    "test:validate": "node scripts/test-validation.js",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "jest --testPathPattern=tests/accessibility",
    "test:build": "npm run build && npm run test:unit",
    "playwright:install": "playwright install",
    "db:migrate": "node scripts/migrate.js",
    "lhci": "lhci autorun",
    "analyze": "npm run build:analyze",
    "prebuild": "npm run check:env && npm run check",
    "postbuild": "npm run health-check"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.5.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-select": "^2.2.6",
    "@types/jsonwebtoken": "^9.0.10",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "minisearch": "^7.1.2",
    "next": "15.5.2",
    "next-intl": "^4.3.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intl": "^7.1.11",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^29.7.0",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^2.0.16",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "express": "^5.1.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "undici": "^6.21.3",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
