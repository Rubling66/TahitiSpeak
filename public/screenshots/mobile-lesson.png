<svg width="512" height="512" viewBox="0 0 512 512" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle -->
  <circle cx="256" cy="256" r="256" fill="#0ea5e9"/>
  
  <!-- Palm tree silhouette -->
  <g transform="translate(256, 256)">
    <!-- Tree trunk -->
    <rect x="-8" y="80" width="16" height="120" fill="#8b4513" rx="8"/>
    
    <!-- Palm fronds -->
    <g transform="translate(0, 80)">
      <!-- Center frond -->
      <ellipse cx="0" cy="-60" rx="12" ry="80" fill="#22c55e"/>
      
      <!-- Left fronds -->
      <ellipse cx="-40" cy="-40" rx="10" ry="60" fill="#22c55e" transform="rotate(-30)"/>
      <ellipse cx="-60" cy="-20" rx="8" ry="50" fill="#16a34a" transform="rotate(-50)"/>
      
      <!-- Right fronds -->
      <ellipse cx="40" cy="-40" rx="10" ry="60" fill="#22c55e" transform="rotate(30)"/>
      <ellipse cx="60" cy="-20" rx="8" ry="50" fill="#16a34a" transform="rotate(50)"/>
      
      <!-- Back fronds -->
      <ellipse cx="-20" cy="-50" rx="8" ry="55" fill="#16a34a" transform="rotate(-15)"/>
      <ellipse cx="20" cy="-50" rx="8" ry="55" fill="#16a34a" transform="rotate(15)"/>
    </g>
    
    <!-- Coconuts -->
    <circle cx="-15" cy="65" r="8" fill="#8b4513"/>
    <circle cx="10" cy="70" r="6" fill="#a0522d"/>
  </g>
  
  <!-- Text elements -->
  <g transform="translate(256, 400)">
    <!-- Letter T -->
    <rect x="-80" y="-20" width="60" height="12" fill="white" rx="6"/>
    <rect x="-56" y="-20" width="12" height="40" fill="white" rx="6"/>
    
    <!-- Letter T -->
    <rect x="-10" y="-20" width="60" height="12" fill="white" rx="6"/>
    <rect x="14" y="-20" width="12" height="40" fill="white" rx="6"/>
  </g>
  
  <!-- Decorative wave at bottom -->
  <path d="M0 450 Q128 430 256 450 T512 450 L512 512 L0 512 Z" fill="#0284c7" opacity="0.7"/>
</svg>